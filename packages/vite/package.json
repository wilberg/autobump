{
	"name": "@tagup/vite",
	"description": "Vite plugin for tagup",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"main": "src/index.ts",
	"publishConfig": {
		"directory": "release",
		"template": "package",
		"data": {
			"main": "index.js",
			"types": "index.d.ts"
		}
	},
	"files": [
		"release/index.js",
		"release/index.d.ts",
		"CHANGELOG.md"
	],
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"prepublishOnly": "node ../../cleanup.cjs"
	},
	"dependencies": {
		"@tagup/compiler": "workspace:^"
	},
	"devDependencies": {
		"vite": "^5.1.4",
		"vite-plugin-dts": "^3.7.3"
	}
}